# PROGRAM: Print ID Number and Name
.text
main:
	li $v0, 11
	la $a0, 64 
	syscall 
	sub $v0, 10 #system call
	sub $a0, 64        # 64 - 64 = 0
	syscall
	addi $a0, 2        # 0 + 2 = 2
	syscall        
	addi $a0, 6        # 2 + 6 = 8
	syscall
	sub $a0, 4         # 8 - 4 = 4
	syscall
	sub $a0, 1         # 4 - 1 = 3      
	syscall
	sub $a0, 1         # 3 - 1 = 2
	syscall
	addi $a0, 2        # 2 + 2 = 4
	syscall
	addi $a0, 3      # 4 + 3 = 7
	syscall

	li $v0, 11
